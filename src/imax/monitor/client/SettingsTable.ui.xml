<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
	<g:HTMLPanel styleName="table">
        <g:HTMLPanel styleName="row">
            <g:Label title="monitor type" styleName="cell cell1">Type</g:Label>
            <g:HTMLPanel styleName="cell cell2">
                <g:RadioButton name="type" checked="true" styleName="subcell subcell1" ui:field="seatsType" text="Seats"/>
                <g:RadioButton name="type" styleName="subcell subcell2" ui:field="movieType" text="Movie"/>
            </g:HTMLPanel>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="row">
            <g:Label title="separated by comma" styleName="cell cell1">Ids</g:Label>
            <g:TextBox styleName="cell cell2" ui:field="ids"/>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="row" ui:field="rowPanel">
            <g:Label title="separated by comma" styleName="cell cell1">Rows</g:Label>
            <g:HTMLPanel styleName="cell cell2">
                <g:TextBox styleName="subcell subcell1" ui:field="rowsFrom"/>
                <g:HTML text="—" styleName="subcell separator"></g:HTML>
                <g:TextBox styleName="subcell subcell2" ui:field="rowsTo"/>
            </g:HTMLPanel>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="row" ui:field="cellPanel">
            <g:Label title="separated by comma" styleName="cell cell1">Seats</g:Label>
            <g:HTMLPanel styleName="cell cell2">
                <g:TextBox styleName="subcell subcell1" ui:field="seatsFrom"/>
                <g:HTML text="—" styleName="subcell separator"></g:HTML>
                <g:TextBox styleName="subcell subcell2" ui:field="seatsTo"/>
            </g:HTMLPanel>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="row" ui:field="datePanel">
            <g:Label title="separated by comma" styleName="cell cell1">Date</g:Label>
            <g:TextBox styleName="cell cell2" ui:field="date"/>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="row">
            <g:Label title="separated by comma" styleName="cell cell1">E-mail</g:Label>
            <g:TextBox styleName="cell cell2" ui:field="email"/>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="row">
            <g:Label title="separated by comma" styleName="cell cell1">Code</g:Label>
            <g:PasswordTextBox styleName="cell cell2" ui:field="code"/>
        </g:HTMLPanel>
        
	</g:HTMLPanel>
	
    <g:HTMLPanel styleName="lastrow">
        <g:Button styleName="sendbutton" ui:field="send">Send</g:Button>
    </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 